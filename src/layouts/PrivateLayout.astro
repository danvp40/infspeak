---
import Layout from '~/layouts/BaseLayout.astro';
import ServiceHeader from '~/components/widgets/ServiceHeader.astro';

import { MetaSEO } from '~/types';

export interface Props {
  meta?: MetaSEO;
}

const { meta } = Astro.props;
---

<Layout {meta}>
  <slot name="header">
    <ServiceHeader />
  </slot>
  <main>
    <slot />
  </main>
  <slot name="footer" />
</Layout>
